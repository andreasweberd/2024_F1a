void setup() {
  // put your setup code here, to run once:
  pinMode(A0, INPUT);

  Serial.begin(9600);
}

void loop() {
  // put your main code here, to run repeatedly:
  int analogWertVomSoundSensor = analogRead(A1);
  Serial.println(analogRead(A0)); //aktuellen Wert von Pin A1 ausgeben
  delay(80); //80ms warten
}
